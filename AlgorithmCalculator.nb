Manipulate[NSolve[binaryRunTime = Log[2, data]], {data, 0, 100, 1}]
db = Dynamic[binaryRunTime]
StringForm["Base 2 logarithm of data size: ``", db]
StringForm["Runtime of binary search of data size (Worst Case): ``", db]
Manipulate[NSolve[selectionSortRunTime = Times[Subtract[Power[nv=n, 2],n]]], {n, 0, 100, 1}]
dn = Dynamic[selectionSortRunTime]
StringForm["Number of elements (nv): ``", Dynamic[nv]]
StringForm["Comparison and progression count of selection sort (n): ``", dn]
Manipulate[NSolve[linearSearchRunTime = cr = n], {n, 0, 1000, 1}]
cv = Dynamic[linearSearchRunTime]
StringForm["Number of elements (cr): ``", Dynamic[cr]]
StringForm["Run-time (worst-case): ``", cv]
